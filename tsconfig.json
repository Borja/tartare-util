{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "sourceMap": false,
        "moduleResolution": "node",
        "pretty": true,
        "noEmit": true
    },
    "exclude": [
        "node_modules"
    ]
}
