  "dependencies": {
    "lodash": "^4.13.1",
    "mustache": "^2.2.1",
    "which": "^1.2.10"
  },
  "devDependencies": {
    "@types/node": "^6.0.41",
    "conventional-changelog-cli": "^1.2.0",
    "eslint": "^2.13.1",
    "eslint-config-google": "^0.6.0",
    "eslint-plugin-jsdoc": "^2.3.1",
    "release-it": "^2.4.3",
    "ts-node": "^1.3.0",
    "tslint": "^3.15.1",
    "tslint-config-typings": "^0.2.4",
    "typescript": "^2.0.3",
    "typings": "^1.4.0"
  }
